{
  "expo": {
    "name": "mobile-new",
    "slug": "mobile-new",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#052333"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#052333"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-font",
        {
          "fonts": [
            {
              "path": "./assets/fonts/openactive.ttf",
              "family": "openactive"
            }
          ]
        }
      ]
    ]
  }
}
